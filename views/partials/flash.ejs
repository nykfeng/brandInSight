<% if(success && success.length) { %> 
<div class="alert alert-success" role="alert">
    <button> X </button>
    <%= success  %> 
</div>
<% } %> 

<% if(error && error.length) { %> 
    <div class="alert alert-error" role="alert">
        <button> X </button>
        <%= error  %> 
    </div>
    <% } %> 